class A:
    def __init__(self,a):
        
        self.a=a
    def __lt__(self,other):
        if(self.a < other.a):
            return "ob1 is less than ob2"
        else:
            return "ob1 is greater than ob2"    

    def __eq__(self,other):
        if(self.a == other.a):
            return "ob1 is equal to ob2"
        else:
            return "Womp Womp My system not working"
